import com.zhenxiang.dcaboy.model.Exchange;

CREATE TABLE apiKey (
  apiKey TEXT NOT NULL PRIMARY KEY,
  exchange TEXT AS Exchange NOT NULL,
  label TEXT UNIQUE NOT NULL,
  secretKey TEXT,
  creationTimestamp INTEGER AS Long NOT NULL,
  readOnly INTEGER AS Boolean DEFAULT NULL
);

selectAll:
    SELECT * FROM apiKey;

insert:
    INSERT INTO apiKey VALUES ?;
